<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.chengchikeji_activity.www.GeRenShezhiActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="96px"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#ED483A"
        android:id="@+id/relativeLayout8">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="个人设置"
            android:textColor="#FFFFFF"
            android:textSize="36px" />

        <RelativeLayout
            android:id="@+id/back_gerenSetting"
            android:layout_width="80px"
            android:layout_height="match_parent"
            android:gravity="center">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="46px"
                android:layout_height="46px"
                android:src="@drawable/back" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_below="@+id/relativeLayout8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout9">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:id="@+id/textView33"
            android:background="#E6E6E6"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="接收通知"
            android:id="@+id/textView38"
            android:textColor="#333333"
            android:textSize="32px"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="30px"
            android:layout_marginStart="30px" />

        <ImageView
            android:layout_width="82px"
            android:layout_height="42px"
            android:id="@+id/imageView14"
            android:layout_marginRight="30px"
            android:layout_marginEnd="30px"
            android:src="@drawable/open_close"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_below="@+id/relativeLayout9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout10">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:id="@+id/textView35"
            android:background="#E6E6E6"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="官方公告"
            android:id="@+id/textView39"
            android:textColor="#333333"
            android:textSize="32px"
            android:layout_marginLeft="30px"
            android:layout_marginStart="30px"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="46px"
            android:layout_height="46px"
            android:id="@+id/imageView17"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="30px"
            android:layout_marginEnd="30px"
            android:src="@drawable/enter_the_next_level" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_below="@+id/relativeLayout10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout11">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:id="@+id/textView36"
            android:background="#E6E6E6"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="常见问题"
            android:id="@+id/textView40"
            android:textColor="#333333"
            android:textSize="32px"
            android:layout_marginLeft="30px"
            android:layout_marginStart="30px"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="46px"
            android:layout_height="46px"
            android:id="@+id/imageView18"
            android:src="@drawable/enter_the_next_level"
            android:layout_alignBottom="@+id/textView40"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="30px"
            android:layout_marginEnd="30px" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_below="@+id/relativeLayout11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout12">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:id="@+id/textView37"
            android:background="#E6E6E6"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除缓存"
            android:id="@+id/textView41"
            android:textColor="#333333"
            android:textSize="32px"
            android:layout_marginLeft="30px"
            android:layout_marginStart="30px"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="46px"
            android:layout_height="46px"
            android:id="@+id/imageView19"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="30px"
            android:layout_marginEnd="30px"
            android:src="@drawable/enter_the_next_level" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="11MB"
            android:id="@+id/textView42"
            android:textSize="32px"
            android:textColor="#888888"
            android:layout_alignTop="@+id/textView41"
            android:layout_toLeftOf="@+id/imageView19"
            android:layout_toStartOf="@+id/imageView19"
            android:layout_marginRight="20px"
            android:layout_marginEnd="20px" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:text="退出登录"
        android:id="@+id/textView43"
        android:textSize="36px"
        android:textColor="#FFFFFF"
        android:gravity="center"
        android:background="@drawable/registration_confirmation_button"
        android:layout_below="@+id/relativeLayout12"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30px"
        android:layout_marginRight="30px"
        android:layout_marginLeft="30px"/>
</RelativeLayout>
